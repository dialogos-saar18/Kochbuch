						<att name="title" type="string" value="Script"/>
						<att name="script" type="text">
							<value>gram = &quot;root $input; \n&quot;;
gram = gram + &quot;$input = $personen {$=$personen} | $zutaten {$=$zutaten};&quot;;
gram = gram + &quot;$personen = [ich möchte] [für] 1 Person kochen {$=[\&quot;Personen\&quot;,\&quot;&quot;+1+&quot;\&quot;]} | [ich möchte] [für|mit] $anz Personen [kochen] {$=[\&quot;Personen\&quot;, $anz]};&quot;;
gram = gram + &quot;$anz = 2&quot;;
for (int i = 3; i&lt;50 ; i++)
	gram = gram + &quot; | &quot; + i ;
gram = gram + &quot;;&quot;;

gram = gram + &quot;$zutaten = [ich möchte] [für] $zahl $einheit $term_z [kochen] {$=[\&quot;Zutaten\&quot;, $term_z, $zahl, $einheit]}| [ich möchte] [für] $zahl $term_z [kochen] {$=[\&quot;Zutaten\&quot;, $term_z, $zahl]};&quot;;
gram = gram + &quot;$term_z = &quot; + get(ingredients,0);
for (int i = 1; i&lt;length(ingredients); i++)
	gram = gram + &quot; | &quot; + get(ingredients,i);
gram = gram + &quot;;&quot;;
gram = gram + &quot;$einheit = &quot; + get(einheiten, 0);
for (int i = 1; i&lt;length(einheiten); i++)
	gram = gram + &quot; | &quot; + get(einheiten,i);
gram = gram + &quot;;&quot;;

gram = gram + &quot;$zahl = $integer {$=$integer} | $komm {$=$komm};&quot;;
gram = gram + &quot;$integer = 1&quot;;
for (int i = 2; i&lt;1001; i++)
	gram = gram + &quot; | &quot; + i;
gram = gram + &quot;; \n&quot;;
gram = gram + &quot;$komm = $integer komma $term_i $term_i2 {$ = $integer + \&quot;komma\&quot; + $term_i + $term_i2;} | $integer komma $term_i {$ = $integer + \&quot;komma\&quot; + $term_i;};&quot;;
gram = gram + &quot;$term_i = 0 &quot;;
for (int i = 1; i &lt; 10; i++)
	gram = gram + &quot; | &quot; + i;
gram = gram + &quot;;&quot;;
gram = gram + &quot;$term_i2 = 0&quot;;
for (int i = 1; i &lt; 10;i++)
	gram = gram + &quot; | &quot; + i;
gram = gram + &quot;;&quot;;</value>
						</att>
