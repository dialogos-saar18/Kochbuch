					<node uid="38075a29-b46f-49db-aa85-fdbc788c4f52" class="de.saar.coli.dialogos.marytts.plugin.TTSNode">
						<graphics x="576" y="158" fill="#00b200"/>
						<att name="title" type="string" value="Sprachsynthese"/>
						<att name="prompt" type="text">
							<value>Im weiteren Verlauf kannst du nach Mengenangaben, Schritten, und weiteren Informationen wie Arbeitszeit oder Schwierigkeitsgrad fragen.</value>
						</att>
