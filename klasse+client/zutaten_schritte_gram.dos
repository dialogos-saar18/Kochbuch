		<grammar id="0bc38a8e-cae8-4b4a-84fa-9be0bfafb1d1">
			<name>zutatenschritte</name>
			<value>root $input;
 $input = $zutaten {$=[&quot;zutaten&quot;, $term_z]}
		| $eigenschaft 
		| $anleitung;

$zutaten = Wie viel $term_z brauche ich ;
$term_z = milch | zucker;

$eigenschaft = Wie lange (braucht | dauert) {$=[&quot;eigenschaft&quot;,&quot;zubereitungszeit&quot;]} 
			| (Wie schwer ist | Welchen Schwierigkeitsgrad) {$=[&quot;eigenschaft&quot;,&quot;schwierigkeitsgrad&quot;]};

$anleitung = Was ist der $term_a Schritt 
			| $term_a Schritt
			| (Wiederhole den Schritt | [diesen Schritt] nochmal {$=&quot;anleitung&quot;,&quot;repeat&quot;});
$term_a = nächste {$=&quot;anleitung&quot;,&quot;next&quot;} | letzte {$=&quot;anleitung&quot;,&quot;previous&quot;};</value>
		</grammar>
		<grammar id="ef0d4498-5c51-46da-8237-16a99f071f91">
			<name>mustertest</name>
			<value>root $input;
 $input = 1 {$= [&quot;eigenschaften&quot;,&quot;eins&quot;]}| 2 {$= [&quot;anleitung&quot;,&quot;zwei&quot;]}| 3{$= [&quot;zutaten&quot;,&quot;drei&quot;]};</value>
		</grammar>
		<grammar id="887988cb-ab10-4c6a-9603-09be63e44e57">
			<name>zutaten_anleitung_weiter</name>
			<value>root $input;
 $input = $zutaten {$=&quot;zutaten&quot;} | $anleitung {$=&quot;anleitung&quot;} | $weiter {$=&quot;weiter&quot;} | $einkaufszettel {$=&quot;einkaufszettel&quot;};
$zutaten = [sag mir | gib mir] (die Liste | alle Zutaten | die Zutatenliste) | [die] Zutaten | [die] Zutatenliste | was brauche ich;
$anleitung = [sag mir | gib mir] ([alle] Schritte | [die] (komplette | ganze) Anleitung) | was muss ich tun ;
$weiter = weiter | [weitere | die] Informationen | überspringen | fortfahren | nein | weder noch | nichts davon | keins von beiden;
$einkaufszettel = [schreib mir einen] Einkaufszettel;</value>
		</grammar>
		<grammar id="9a5dcd6a-8229-4015-b7ba-cd13e10b853f">
			<name>janein+</name>
			<value>//Sprache
