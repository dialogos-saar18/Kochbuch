					<node uid="56aa9472-a449-411f-be8e-64c043d9c6dc" class="com.clt.diamant.graph.nodes.ScriptNode">
						<graphics x="567" y="84" fill="#99ffff"/>
						<att name="title" type="string" value="Workaround (dynamische Grammatik)"/>
						<att name="comment" type="text">
							<value>Die Grammatik ist sehr hässlich, da einzeilig
</value>
						</att>
						<att name="script" type="text">
							<value>dyngram = &quot;root $input; \n&quot;;
dyngram = dyngram + &quot;$input = (ende | fertig) {$=\&quot;ende\&quot;} | $ruhezeit {$=[\&quot;eigenschaft\&quot;,\&quot;Ruhezeit\&quot;]} | $arbeitszeit {$=[\&quot;eigenschaft\&quot;,\&quot;Arbeitszeit\&quot;]} | $kochzeit {$=[\&quot;eigenschaft\&quot;,\&quot;Koch-/Backzeit\&quot;]} | $schwierig {$=[\&quot;eigenschaft\&quot;,\&quot;Schwierigkeitsgrad\&quot;]} | $kalorien {$=[\&quot;eigenschaft\&quot;,\&quot;Kalorien\&quot;]} | $anleitung {$=$anleitung} | $zutaten_menge {$=$zutaten_menge} | $zutaten_exist {$=$zutaten_exist};&quot;;
dyngram = dyngram + &quot;$ruhezeit = wie viel Ruhezeit (muss | kann | soll) ich rechnen | was ist die Ruhezeit | wie lange muss es ruhen;&quot;;
dyngram = dyngram + &quot;$arbeitszeit = wie viel Arbeitszeit (muss | kann | soll) ich rechnen | was ist die Arbeitszeit;&quot;;
dyngram = dyngram + &quot;$kochzeit = wie viel (Kochzeit | Backzeit) (muss | kann | soll) ich rechnen | was ist die (Kochzeit | Backzeit) | wie lange (kocht | backt) es | wie lange muss es (kochen | backen);&quot;;
dyngram = dyngram + &quot;$schwierig = wie schwer ist (es | das Rezept) | welchen Schwierigkeitsgrad hat (es | das Rezept);&quot;;
dyngram = dyngram + &quot;$kalorien = wie viele Kalorien hat das (Gericht | Essen);&quot;;

dyngram = dyngram + &quot;$anleitung = $next {$=[\&quot;anleitung\&quot;,\&quot;next\&quot;]} | $prev {$=[\&quot;anleitung\&quot;,\&quot;previous\&quot;]} | $first {$=[\&quot;anleitung\&quot;,\&quot;first\&quot;]} | $rep {$=[\&quot;anleitung\&quot;,\&quot;repeat\&quot;]};&quot;;
dyngram = dyngram + &quot;$next = was ist der nächste Schritt | nächster Schritt;&quot;;
dyngram = dyngram + &quot;$prev = was ist der letzt Schritt | letzter Schritt;&quot;;
dyngram = dyngram + &quot;$first = erster Schritt;&quot;;
dyngram = dyngram + &quot;$rep = was ist der erste Schritt | [Wiederhole (den | diesen) Schritt] nochmal | wiederholen;&quot;;

dyngram = dyngram + &quot;$zutaten_exist = brauche ich $term_z {$=[\&quot;exists_zutat\&quot;, $term_z]} | (kommt | kommen | gehört | gehören) $term_z (rein | hinein | dazu | dabei) {$=[\&quot;exists_zutat\&quot;, $term_z]};&quot;;

dyngram = dyngram + &quot;$zutaten_menge = wie viel $term_z brauche ich {$=[\&quot;zutaten\&quot;, $term_z]};&quot;;
dyngram = dyngram + &quot;$term_z = &quot; + get(ingredients,0);
for (int i = 1; i&lt;length(ingredients); i++)
  dyngram = dyngram + &quot; | &quot; + get(ingredients,i);
dyngram = dyngram + &quot;;&quot;;</value>
						</att>
