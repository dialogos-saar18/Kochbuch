		<grammar id="0bc38a8e-cae8-4b4a-84fa-9be0bfafb1d1">
			<name>zutatenschritte</name>
			<value>root $input;
 $input = $zutaten {$=[&quot;zutaten&quot;, $term_z]}
		| $eigenschaft 
		| $anleitung;

$zutaten = Wie viel $term_z brauche ich ;
$term_z = milch | zucker;

$eigenschaft = Wie lange (braucht | dauert) {$=[&quot;eigenschaft&quot;,&quot;zubereitungszeit&quot;]} 
			| (Wie schwer ist | Welchen Schwierigkeitsgrad) {$=[&quot;eigenschaft&quot;,&quot;schwierigkeitsgrad&quot;]};

$anleitung = Was ist der $term_a Schritt 
			| $term_a Schritt
			| (Wiederhole den Schritt | [diesen Schritt] nochmal {$=&quot;anleitung&quot;,&quot;repeat&quot;});
$term_a = n√§chste {$=&quot;anleitung&quot;,&quot;next&quot;} | letzte {$=&quot;anleitung&quot;,&quot;previous&quot;};</value>
		</grammar>
