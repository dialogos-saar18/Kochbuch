			<att name="script" type="text">
				<value>gram = &quot;root $input; \n&quot;;
gram = gram + &quot;$input = &quot;;
gram = gram + &quot;(ich möchte für) 1 Person kochen {$=[\&quot;Personen\&quot;,1]}&quot;;
for (int i=2; i&lt;99; i++)
  gram = gram + &quot;| (ich möchte) für &quot; + i + &quot; Personen kochen {$=[\&quot;Personen\&quot;,\&quot;&quot;+ i +&quot;\&quot;]}&quot;;
for (int i = 0; i &lt; 15; i++){
	for (int n = 0; n&lt;length(ingredients); n++){
		for (int m=0; m&lt;length(einheiten); m++)
			gram = gram + &quot;| (ich möchte) (für)&quot; + i + get(einheiten,m) + get(ingredients,n) + &quot;{$=[\&quot;Zutaten\&quot;,\&quot;&quot; + get(ingredients,n) +&quot;\&quot;,\&quot;&quot; + i + &quot;\&quot;,\&quot;&quot; + get(einheiten,m) + &quot;\&quot;]}&quot;;
}
}
gram = gram + &quot;;&quot;;</value>
